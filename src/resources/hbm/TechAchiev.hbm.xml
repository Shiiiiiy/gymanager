<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.uws.model.TechAchiev" table="TECH_ACHIEV">
        <id name="id" column="ID">
	      <generator class="uuid.hex"/>
	    </id>
        <property name="name" type="string" column="NAME"/>
        <property name="belong" type="string" column="BELONG"/>
        <property name="level" type="string" column="LEVEL"/>
        <property name="certTime" type="timestamp" column="CERT_TIME"/>
        <property name="effectTime" type="timestamp" column="EFFECT_TIME"/>
        <property name="type" type="string" column="TYPE"/>
        <property name="comments" type="string" column="COMMENTS"/>
        <property name="status" type="int" column="STATUS"/>
        <property name="createTime" type="timestamp" column="CREATE_TIME"/>
    </class>
</hibernate-mapping>
