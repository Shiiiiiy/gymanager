<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.uws.model.Dic" table="SYS_DIC">
        <id name="id" type="long">
            <column name="ID" length="20" />
            <generator class="identity" />
        </id>

        <property name="name" type="java.lang.String" column="NAME"/>
        <property name="code" type="java.lang.String" column="CODE"/>
        <property name="value" type="java.lang.String" column="VAL"/>
        <property name="status"  type="java.lang.Long" column="STATUS"/>
        <property name="remark" type="java.lang.String" column="REMARK"/>     
        <property name="pid" column="P_ID" type="java.lang.Long" />
        <property name="recTime" type="timestamp" column="REC_TIME"/>
        <property name="updateTime" type="timestamp" column="UPDATE_TIME"/>
        
    </class>
</hibernate-mapping>
