<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.uws.model.InnovBusiness" table="INNOV_BUSINESS">
        <id name="id" column="ID">
	      <generator class="uuid.hex"/>
	    </id>
        <property name="name" type="string" column="NAME"/>
        <property name="belong" type="string" column="BELONG"/>

        <property name="scale" type="string" column="SCALE"/>
        <property name="startTime" type="timestamp" column="START_TIME"/>
        
        <property name="linkman" type="string" column="LINKMAN"/>
        <property name="phone" type="string" column="PHONE"/>
        <property name="url" type="string" column="URL"/>
        
        
        <property name="status" type="int" column="STATUS"/>
        <property name="createTime" type="timestamp" column="CREATE_TIME"/>
        
        <property name="comments" type="string" column="COMMENTS"/>
    </class>
</hibernate-mapping>
