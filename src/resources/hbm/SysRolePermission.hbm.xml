<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.uws.model.SysRolePermission" table="REF_ROLE_PERMISSION">
        <id name="id" type="long">
            <column name="ID" length="20" />
            <generator class="identity" />
        </id>
		<property column="ROLE_ID" name="roleId"  type="java.lang.Long" />
		<property column="TYPE" name="type"  type="string" />
		<property column="VALUE" name="value"  type="string" />
		
    </class>
</hibernate-mapping>
