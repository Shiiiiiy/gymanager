<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.uws.model.TechInnovation" table="TECH_INNOVATION">
        <id name="id" column="ID">
	      <generator class="uuid.hex"/>
	    </id>
        <property name="name" type="string" column="NAME"/>
        <property name="belong" type="string" column="BELONG"/>
        <property name="level" type="string" column="LEVEL"/>
        <property name="scale" type="string" column="SCALE"/>
        <property name="startTime" type="timestamp" column="START_TIME"/>
        <property name="comments" type="string" column="COMMENTS"/>
        <property name="status" type="int" column="STATUS"/>
        <property name="createTime" type="timestamp" column="CREATE_TIME"/>
    </class>
</hibernate-mapping>
