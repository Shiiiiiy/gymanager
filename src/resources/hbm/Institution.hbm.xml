<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.uws.model.Institution" table="Institution">
        <id name="id" type="string">
            <column name="ID" length="32" />
            <generator class="uuid" />
        </id>
        <property name="userId" type="string" column="USERID"/>
        <property name="name" type="string" column="NAME"/>
        <property name="linkman" type="string" column="LINKMAN"/>
        <property name="createTime" type="timestamp" column="CREATETIME"/>
        <property name="updateTime" type="timestamp" column="UPDATETIME"/>
        <property name="tel" type="string" column="TEL"/>
        <property name="phone" type="string" column="PHONE"/>
        <property name="email" type="string" column="EMAIL"/>
        <property name="orgCode" type="string" column="ORGCODE"/>
        <property name="socialCode" type="string" column="SOCIALCODE"/>
        <property name="address" type="string" column="ADDRESS"/>
        <property name="province" type="string" column="PROVINCE"/>
        <property name="city" type="string" column="CITY"/>
        <property name="area" type="string" column="AREA"/>
        <property name="status" type="long" column="STATUS"/>
    </class>
</hibernate-mapping>
